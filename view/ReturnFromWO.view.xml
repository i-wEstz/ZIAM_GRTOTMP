<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.mrpReportGr.controller.ReturnFromWO" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
    <Page showHeader="true" title="Returned Work Order" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
        <content>
            <form:Form width="100%" editable="true" visible="true">
                <form:title>
                    <core:Title text="W/O Information" level="Auto"/>
                </form:title>
                <form:formContainers>
                    <form:FormContainer visible="true">
                        <form:formElements>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="W/O Number" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                                </form:label>
                                <form:fields>
                                    <Input placeholder="Scan or Key-in" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" maxLength="0"/>
                                    <Button text="" type="Default" icon="sap-icon://bar-code" iconFirst="true" enabled="true" visible="true" iconDensityAware="false"/>
                                </form:fields>
                            </form:FormElement>
                        </form:formElements>
                        <form:title/>
                    </form:FormContainer>
                </form:formContainers>
                <form:layout>
                    <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                </form:layout>
            </form:Form>
            <Panel height="auto" width="100%" headerText="More" expandable="true" expanded="false" expandAnimation="true" backgroundDesign="Solid">
                <content>
                    <form:Form width="100%" editable="true" visible="true">
                        <form:title/>
                        <form:formContainers>
                            <form:FormContainer visible="true">
                                <form:formElements>
                                    <form:FormElement visible="true">
                                        <form:label>
                                            <Label text="Note" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                                        </form:label>
                                        <form:fields>
                                            <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" maxLength="0"/>
                                            <Button text="" type="Default" icon="sap-icon://edit" iconFirst="true" width="50px" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement visible="false">
                                        <form:label>
                                            <Label text="Account" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                                        </form:label>
                                        <form:fields>
                                            <Button text="" type="Default" icon="sap-icon://kpi-managing-my-area" iconFirst="true" width="50px" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress1"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement visible="false">
                                        <form:label>
                                            <Label text="MVT" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="Default" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                                <form:title/>
                            </form:FormContainer>
                        </form:formContainers>
                        <form:layout>
                            <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                        </form:layout>
                    </form:Form>
                </content>
            </Panel>
            <Table width="auto" noDataText="No data" mode="Delete" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" class="sapUiResponsiveMargin" delete="_onTableDelete">
                <infoToolbar>
                    <Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
                        <content>
                            <Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                        </content>
                    </Toolbar>
                </infoToolbar>
                <headerToolbar>
                    <Toolbar width="auto" height="46px" design="Transparent" visible="true" enabled="true">
                        <content>
                            <Title text="Items (3)" titleStyle="Auto" width="auto" textAlign="Begin" visible="true"/>
                            <ToolbarSpacer width=""/>
                        </content>
                        <tooltip/>
                    </Toolbar>
                </headerToolbar>
                <columns>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Item" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Material" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Batch" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Good Receive" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Ordered/Remaining" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                        <header>
                            <Text text="Unit of Measure" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </header>
                        <footer/>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem type="Active">
                        <cells>
                            <ObjectIdentifier title="0010" text="" titleActive="false" visible="true"/>
                            <ObjectIdentifier title="332-213" text="Material Name (valuation class)" titleActive="true" visible="true" titlePress="_onObjectIdentifierTitlePress"/>
                            <Input value="Default" placeholder="Default" type="Text" showValueHelp="true" enabled="true" visible="true" width="180px" valueHelpOnly="false" maxLength="0"/>
                            <StepInput value="0" min="0" max="10" step="1" largerStep="2" displayValuePrecision="0" width="130px" valueState="None" editable="true" enabled="true"/>
                            <Label text="10/10" design="Standard" width="100%" required="false" textAlign="Center" textDirection="Inherit"/>
                            <Text text="EA" width="auto" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit"/>
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem type="Active">
                        <cells>
                            <ObjectIdentifier title="0010" text="" titleActive="false" visible="true"/>
                            <ObjectIdentifier title="332-214" text="Material Name" titleActive="true" visible="true" titlePress="_onObjectIdentifierTitlePress1"/>
                            <Input value="Default" type="Text" showValueHelp="true" enabled="false" visible="true" width="180px" valueHelpOnly="true" maxLength="0"/>
                            <StepInput value="0" min="0" max="10" step="1" largerStep="2" displayValuePrecision="0" width="130px" valueState="None" editable="true" enabled="true"/>
                            <Label text="20/20" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                            <Text text="EA" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
        <footer>
            <Bar design="Auto">
                <contentLeft>
                    <Button text="Clear All" type="Reject" icon="sap-icon://repost" iconFirst="true" width="150px" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress2"/>
                </contentLeft>
                <contentMiddle/>
                <contentRight>
                    <Button text="Confirm" type="Accept" icon="sap-icon://sys-enter-2" iconFirst="true" width="150px" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress3"/>
                </contentRight>
            </Bar>
        </footer>
        <headerContent>
            <Button text="" type="Default" icon="sap-icon://collections-insight" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress4"/>
        </headerContent>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
